      <dependency>
        <groupId>io.zipkin.zipkin2</groupId>
        <artifactId>zipkin-collector-kafka08</artifactId>
        <version>${project.version}</version>
      </dependency>

      <dependency>
        <groupId>io.zipkin.zipkin2</groupId>
        <artifactId>zipkin-collector-kafka</artifactId>
